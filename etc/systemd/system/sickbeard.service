[Unit]
Description=SickBeard Daemon
After=network.target openvpn@client.service

[Service]
EnvironmentFile=/etc/sysconfig/sickbeard
User=transmission
Group=transmission
Type=forking
ExecStart=/usr/bin/python2.7.14 /data/sickbeard/SickBeard.py $DAEMON_ARGS
PIDFile=/run/mediaservices/sickbeard.pid
GuessMainPID=no
Restart=always
RestartSec=5
Nice=-20

[Install]
WantedBy=multi-user.target